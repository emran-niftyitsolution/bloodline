{
  "name": "my-expo-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "prebuild:android": "expo prebuild --platform android --clean",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:apk": "bun run prebuild:android && bun run build:android",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "build:ios": "cd ios && xcodebuild -workspace Rently.xcworkspace -scheme Rently -configuration Release -sdk iphoneos -archivePath build/Rently.xcarchive archive CODE_SIGN_IDENTITY=\"\" CODE_SIGNING_REQUIRED=NO",
    "build:ios:app": "bun run prebuild:ios && bun run build:ios"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "8.4.4",
    "expo": "~54.0.30",
    "expo-constants": "~18.0.12",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-navigation-bar": "^5.0.10",
    "expo-router": "~6.0.21",
    "expo-status-bar": "~3.0.9",
    "nativewind": "latest",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "~5.9.2"
  },
  "main": "expo-router/entry",
  "private": true
}
